{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product"
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "ProguctImg": {
      "allowedTypes": [
        "files",
        "images"
      ],
      "type": "media",
      "multiple": true
    },
    "ProductTitle": {
      "type": "string",
      "required": true
    },
    "ProguctPrice": {
      "type": "decimal",
      "required": true
    },
    "ProguctDecription": {
      "type": "text",
      "required": true
    },
    "ProguctRating": {
      "type": "decimal",
      "required": true
    },
    "ProguctCategory": {
      "type": "enumeration",
      "enum": [
        "Men ",
        "Women"
      ],
      "required": true
    }
  }
}
